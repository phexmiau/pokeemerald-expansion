script RegionalVenue_EventScript_Stonjourner {
    lock
    faceplayer
    playmoncry(SPECIES_STONJOURNER, CRY_MODE_ENCOUNTER)
    if(flag(FLAG_STARTED_REGIONAL) == FALSE) {
        msgbox("Do you want to start the regional?", MSGBOX_YESNO)
        if(var(VAR_RESULT) == YES) {
            closemessage
            applymovement(9, moves(walk_left * 3))
            waitmovement
            applymovement(9, moves(face_down))
            setflag(FLAG_STARTED_REGIONAL)
            setflag(FLAG_DISABLED_TEAM_CHANGES)
            setobjectxyperm(9, 6, 2)
            msgbox(format("ANNOUNCER: The regional is officially starting!\pPlease make your way to your first game!\pAs a reminder we are in the new Regulation R, which means you are going to play a BO1 with all 6 Pokémon, including 1 restricted.\pBest of luck to all the participants!"), MSGBOX_AUTOCLOSE)
            waitmessage
        }
        if(var(VAR_RESULT) == NO) {
            applymovement(9, moves(face_down))
            waitmovement
        } 
    } else {
        msgbox("Stooooone...")
        msgbox(format("Do you want to enable team customization again?"), MSGBOX_YESNO)
        if(var(VAR_RESULT) == YES) {
            clearflag(FLAG_DISABLED_TEAM_CHANGES)
            setflag(FLAG_STONE_DEAD)
            playmoncry(SPECIES_STONJOURNER, CRY_MODE_ENCOUNTER)
            applymovement(9, moves(walk_in_place_right walk_in_place_up walk_in_place_left walk_in_place_down))
            waitmovement
            applymovement(9, moves(walk_in_place_faster_right walk_in_place_faster_up walk_in_place_faster_left walk_in_place_faster_down))
            waitmovement
            removeobject(9)
        }
        if(var(VAR_RESULT) == NO) {
            applymovement(9, moves(face_down))
            waitmovement
        } 
    }
    release
}

script RegionalVenue_EventScript_MoveStonjourner {
    if(flag(FLAG_STARTED_REGIONAL) == TRUE) {
        setobjectxyperm(9, 6, 2)
        setobjectxy(9, 6, 2)
    }
}

