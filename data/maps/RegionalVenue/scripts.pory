script RegionalVenue_EventScript_Kate {
    lock
    faceplayer
    playmoncry(SPECIES_SPRIGATITO, CRY_MODE_ENCOUNTER)
    switch (var(VAR_REGIONAL_STATE)) {
        case 0:
            msgbox(format("KATE: omg hiiiiiiiiiiiii!!!\NI'm so glad you made it here :3\pIf it's taffel playing ily ily ily, if it's someone else just ignore this :P\pThe regional is about to start tho!!! You should probably prepare your team using the items you can get from the shops here.\NI've heard the guy with the Mismagius on the other side of this room also has some interesting stuff to offer...\pWith all that said... starting now, whenever you talk to me I'm gonna give you some advice on your next tourney matchup :3\pGood luck in the regional !!!"))
            waitmessage
            setvar(VAR_REGIONAL_STATE, 1)
